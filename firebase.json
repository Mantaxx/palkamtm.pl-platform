{
    "hosting": {
        "source": ".",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "function": "nextjsFunc"
            }
        ]
    },
    "functions": {
        "source": "functions",
        "runtime": "nodejs18"
    }
}